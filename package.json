{
  "name": "edumate-graphql",
  "scripts": {
    "start": "nodemon -e js,graphql -x node -r dotenv/config src/index.js",
    "debug": "nodemon -e js,graphql -x node --inspect -r dotenv/config src/index.js",
    "playground": "graphql playground",
    "dev": "npm-run-all --parallel start playground"
  },
  "dependencies": {
    "aws-sdk": "^2.205.0",
    "bcryptjs": "2.4.3",
    "cors": "^2.8.4",
    "graphql-yoga": "1.4.3",
    "jsonwebtoken": "8.2.0",
    "npm": "^5.7.1",
    "prisma-binding": "1.5.15",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "dotenv": "5.0.1",
    "graphql-cli": "2.15.8",
    "nodemon": "1.17.1",
    "npm-run-all": "4.1.2",
    "prisma": "1.3.2"
  }
}
